{
  "name": "@aiter/cli",
  "version": "0.1.15",
  "description": "CLI tool for scaffolding aiter applications and agents",
  "type": "module",
  "keywords": [
    "ai",
    "chat",
    "terminal",
    "tui",
    "mcp",
    "model-context-protocol",
    "cli",
    "scaffold",
    "generator"
  ],
  "homepage": "https://github.com/pranftw/aiter",
  "repository": {
    "type": "git",
    "url": "https://github.com/pranftw/aiter.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/pranftw/aiter/issues"
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "bin": {
    "aiter": "./dist/index.js"
  },
  "files": [
    "dist",
    "template"
  ],
  "publishConfig": {
    "directory": "dist"
  },
  "scripts": {
    "build": "bun run ../../scripts/build-pkg.ts --pkg cli",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.2.0",
    "chalk": "^5.3.0",
    "fs-extra": "^11.2.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.5",
    "@types/yargs": "^17.0.33",
    "typescript": "^5.9.2"
  }
}